    csvFields: [{ name: "LOINC Test Descr" }]

  - name: test_result
    csvFields: [{ name: "SNOMED Rslt Code" }]

  - name: specimen_id
    csvFields: [{ name: "Sample ID" }]

  - name: specimen_collection_date_time
    csvFields: [{ name: "Draw Date" }]

  # todo: needs a mapper
  - name: sample_type
    type: TEXT
    csvFields: [{ name: "Sample Type" }]

  - name: first_test
    csvFields: [{ name: "1st Test?" }]

  - name: employed_in_healthcare
    csvFields: [{ name: "HCW?" }]

  - name: symptomatic_for_disease
    csvFields: [{ name: "Symptomatic" }]

  - name: illness_onset_date
    csvFields: [{ name: "Sympt Date" }]

  - name: hospitalized
    csvFields: [{ name: "Hosp?" }]

  - name: icu
    csvFields: [{ name: "ICU?" }]

