description: A COVID-19 schema for TCP working through A6
trackingElement: specimen_id
topic: covid-19
extends: covid-19
elements:
  - name: patient_id
    csvFields: [ { name: "Patient ID" } ]

  # todo: needs a mapper to split
  - name: patient_name
    type: TEXT
    csvFields: [ { name: Name } ]

  - name: patient_dob
    csvFields: [ { name: DOB } ]

  - name: patient_age
    csvFields: [ { name: Age } ]

  - name: patient_race
    csvFields: [{ name: Race }]

  # todo: needs a mapper
  - name: ordering_provider
    type: TEXT
    csvFields: [{ name: Provider }]

  - name: patient_street
    csvFields: [{ name: Address }]

  - name: patient_city
    csvFields: [{ name: City }]

  - name: patient_state
    csvFields: [{ name: State }]

  - name: patient_zip_code
    csvFields: [{ name: Zip }]

  - name: patient_phone_number
    csvFields: [{ name: Phone }]

  # todo: needs a mapper
  - name: test_result_description
    type: TEXT
    csvFields: [{ name: "Result Descr" }]

  # todo: what should this map to?
