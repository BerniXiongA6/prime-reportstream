---
permalink: "{{ page.filePathStem }}.html" # prefer named files, not blog-like permalinks
---
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=Edge" />
  <meta name="viewport" content="width=device-width, initial-scale=1" />

  {% if title %}
  <title>{{title}} - CDC PRIME Data Hub</title>
  {% else %}
  <title>CDC PRIME Data Hub</title>
  {% endif %}

  <meta name="robots" content="none" />

  <link rel="stylesheet" type="text/css"
    href="https://cdnjs.cloudflare.com/ajax/libs/uswds/2.10.0/css/uswds.min.css"
    integrity="sha512-sL+HPuomAmkkQJsmqQN6RDNe926lTzNBQbRsEMq1dnGU1zI8Qke2pab7A5+kRr3nmYxuONtFhicbS5Y2uNkZWQ=="
    crossorigin="anonymous"/>

  {% for entry in cdn_styles %}
    <link rel="stylesheet" type="text/css"
      href="{{entry.href}}"
      integrity="{{entry.integrity}}"
      crossorigin="anonymous"/>
  {% endfor %}

  {% for entry in local_styles %}
    <link rel="stylesheet" type="text/css"
      href="{{entry | url}}" />
  {% endfor %}

  <script
    src="https://cdnjs.cloudflare.com/ajax/libs/uswds/2.10.0/js/uswds-init.min.js"
    integrity="sha512-Zy/dqU71uCdMWS6VdSqc/+2RjOxbq/ySinrQyLG3wTVWSXtBQpK6/b2Ky6vT2JHuFsNm8yBZMJ8zeMQ/ji2qMA=="
    crossorigin="anonymous"></script>

  {% for entry in cdn_scripts %}
    <script
      src="{{entry.src}}"
      integrity="{{entry.integrity}}"
      crossorigin="anonymous"></script>
  {% endfor %}
</head>

<body>
  {{ content }}

  <script
    src="https://cdnjs.cloudflare.com/ajax/libs/uswds/2.10.0/js/uswds.min.js"
    integrity="sha512-ohEb4+2we68Bx+r0bzmujZwPVxjs+cD0lRykggDCj36aoqO9Tex8TaNO3gzBr8re+MVbbF5Av/d7VDQxUe+zlw=="
    crossorigin="anonymous"></script>

  {% for entry in cdn_post_scripts %}
    <script
      src="{{entry.src}}"
      integrity="{{entry.integrity}}"
      crossorigin="anonymous"></script>
  {% endfor %}
</body>
