<div class="usa-overlay"></div>
<header class="usa-header usa-header--basic">
  <div class="usa-nav-container">
    <div class="usa-navbar">
      <div class="usa-logo" id="basic-logo">
        <em class="usa-logo__text">
          <a href="{{ '/' | url }}" title="Home" aria-label="Home">PRIME Data Hub</a>
        </em>
      </div>
    </div>
    <nav aria-label="Primary navigation" class="usa-nav">
      <button class="usa-nav__close">
        <img
          src="data:image/png;base64,"
          role="img"
          alt="close"
        />
      </button>
      <div class="prime-user-account">
        <span><script>{
          let jwt_body = JSON.parse(localStorage.getItem('jwt_body') || null)
          if (jwt_body) {
            document.currentScript
              .parentElement.textContent = `${jwt_body.sub}`
          } else {
            window.location.replace({{"/login.html" | url | as_literal }})
          }
        }</script></span>

        <br />
        <a href="#" onclick="false">Logout
          <script>
            document.currentScript.parentElement.onclick = (event) => {
              event.preventDefault()

              document.cookie = `jwt=;path=/;expires=${new Date(0).toUTCString()}`
              window.localStorage.removeItem('jwt')
              window.localStorage.removeItem('jwt_body')

              window.location.replace({{"/login.html" | url | as_literal }})
            }
          </script>
        </a>
      </div>
    </nav>
  </div>
</header>
