{% if cdn_styles %}
  {% for entry in cdn_styles %}
    <link rel="stylesheet" type="text/css" href="{{entry.href}}" integrity="{{entry.integrity}}" crossorigin="anonymous"/>
  {% endfor %}
{% endif %}

{% if local_styles %}
  {% for entry in local_styles %}
    <link rel="stylesheet" type="text/css" href="{{entry | url}}" />
  {% endfor %}
{% endif %}

{% if cdn_scripts %}
  {% for entry in cdn_scripts %}
    <script src="{{entry.src}}" integrity="{{entry.integrity}}" crossorigin="anonymous"></script>
  {% endfor %}
{% endif %}
